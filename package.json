{
  "name": "mia-survey-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build2": "ng build --prod --project=@dorotea/mia-survey-core && cd dist/dorotea/mia-survey-core/ && npm publish --access public",
    "build3": "ng build --prod --project=@agencycoda/mia-survey-panel && cd dist/agencycoda/mia-survey-panel/ && npm publish --access public",
    "build4": "ng build --prod --project=@dorotea/mia-survey-viewer && cd dist/dorotea/mia-survey-viewer/ && npm publish --access public",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.4.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.4.0",
    "@angular/compiler": "~13.4.0",
    "@angular/core": "~13.4.0",
    "@angular/forms": "~13.4.0",
    "@angular/material": "^13.3.9",
    "@angular/material-moment-adapter": "^13.3.9",
    "@angular/platform-browser": "~13.4.0",
    "@angular/platform-browser-dynamic": "~13.4.0",
    "@angular/router": "~13.4.0",
    "moment": "^2.29.1",
    "ngx-quill": "^14.1.2",
    "quill": "^1.3.7",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
